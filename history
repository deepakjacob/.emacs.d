;; Minibuffer history file, automatically generated by ‘savehist’.

(setq savehist-minibuffer-history-variables '(file-name-history buffer-name-history command-history evil-ex-history extended-command-history))
(setq file-name-history '("~/.emacs.d"))
(setq command-history '((evil-delete 2474 2504 'line) (menu-bar-mode 'toggle) (execute-extended-command nil "menu-bar-mode" "m") (evil-delete 1783 1809 'line) (marginalia-mode 'toggle) (execute-extended-command nil "marginalia-mode" "margi")))
(setq evil-ex-history '(#("wqa" 0 1 (ex-index 1) 1 2 (ex-index 2) 2 3 (ex-index 3)) #("w" 0 1 (ex-index 1)) #("wqa" 0 1 (ex-index 1) 1 2 (ex-index 2) 2 3 (ex-index 3))))
(setq extended-command-history '("menu-bar-mode" "marginalia-mode" "menu-bar-mode"))
(setq evil-jumps-history 'nil)
